  "version": "5.3.0-pre.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Base for the other Twurple EventSub packages",
  "keywords": [
    "twitch",
    "eventsub",
    "stream",
    "bot",
    "events"
