{
  "name": "@twurple/auth",
  "version": "6.1.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Authenticate with Twitch and stop caring about refreshing tokens.",
  "keywords": [
    "twitch",
    "auth",
    "authentication",
    "oauth",
    "stream",
    "bot"
  ],
  "sideEffects": false,
  "main": "lib",
  "types": "lib",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twurple/twurple.git",
    "directory": "packages/auth"
  },
  "homepage": "https://twurple.js.org",
  "author": "Daniel Fischer <daniel@d-fischer.dev>",
  "funding": "https://github.com/sponsors/d-fischer",
  "license": "MIT",
  "dependencies": {
    "@d-fischer/logger": "^4.2.1",
    "@d-fischer/shared-utils": "^3.4.0",
    "@twurple/api-call": "6.1.1",
    "@twurple/common": "6.1.1",
    "tslib": "^2.0.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "!lib/**/*.d.ts.map",
    "es",
    "!es/**/*.d.ts",
    "!es/**/*.d.ts.map"
  ]
}
